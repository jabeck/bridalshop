<div class="prepend-1">
<h1>Vendors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Id</th>
  </tr>

<% @vendors.each do |vendor| %>
  <tr>
    <td><%=h vendor.name %></td>
    <td><%=h vendor.id %></td>
    <td><%= link_to 'Show', vendor %></td>
    <td><%= link_to 'Edit', edit_vendor_path(vendor) %></td>
    <td><%= link_to 'Destroy', vendor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New vendor', new_vendor_path %>
</div>